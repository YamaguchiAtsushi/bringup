<?xml version="1.0"?>
<launch>
    <node name="waypoint_publisher" pkg="waypoint_publisher" type="waypoint_publisher" output="screen"/>
    <node name="sub_waypoint_publisher" pkg="sub_waypoint_publisher" type="sub_waypoint_publisher" output="screen"/>
    <!-- <node name="state_manager" pkg="state_manager" type="state_manager" output="screen"/> -->
    <node name="collision_monitor" pkg="collision_monitor" type="collision_monitor" output="screen"/>
    <node name="go_waypoint" pkg="go_waypoint" type="go_waypoint" output="screen"/>
    <node name="goal_judge" pkg="goal_judge" type="goal_judge" output="screen"/>

    <node pkg="urg_node" type="urg_node" name="urg_node">
        <param name="serial_port" value="/dev/serial/by-id/usb-Hokuyo_Data_Flex_for_USB_URG-Series_USB_Driver-if00" />
    </node>

    <node pkg="ypspur_ros" type="ypspur_ros" name="ypspur_ros" respawn="true">
    <param name="param_file"
        value="/home/yamaguchi-a/researches/programs/platform/yp-robot-params/robot-params/beego.param" />
    <param name="port" value="/dev/serial/by-id/usb-T-frog_project_T-frog_Driver-if00" />
    <param name="cmd_vel_expire" value="5.0" />
    </node>

    <!-- rvizノード -->
    <!-- <node pkg="rviz" type="rviz" name="rviz" args="-d $(find lidar_clustering)/config/rviz/lidar_clustering.rviz" /> -->

</launch>
